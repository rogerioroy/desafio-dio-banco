package com.banco;

public class Main {

	public static void main(String[] args) {
		
		Conta conta = new Conta();
		
		Cliente cliente = new Cliente();
		cliente.incluirCliente("Rogerio" , "12345678901");
		conta.incluirConta();
		cliente.incluirCliente("Sara" , "99999999999");
		conta.incluirConta();
		
//		cliente.listarCliente();
		cliente.mostraCliente(0);
		
		System.out.println(conta);
		

	}





}
